        [
            {
                "id": "939244311122444319",
                "type": 19,
                "content": "I was thinking of doing this for the Hackathon if it didn't already exist and somebody else thought it would be useful. I was thinking of a generic nft coupon standard that merchants would transfer to potential customers on their websites and then work out the integration with the pos app to reflect the discount in the qualifying purchase tx. You would offer merchants a higher level api that they could use to create collections with fixed supplies and keep track of issuance and use in purchase txs.",
                "channel_id": "938828592232693820",
                "author": {
                    "id": "533682398903402497",
                    "username": "Caleb",
                    "avatar": "a459123bc3fb15fdbaecaca6f17f5863",
                    "avatar_decoration": null,
                    "discriminator": "9071",
                    "public_flags": 0
                },
                "attachments": [],
